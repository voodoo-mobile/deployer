{
  "name": "voodoo-rocks/deployer",
  "description": "Deployment Tool Fork for Voodoo Rocks",
  "license": "MIT",
  "homepage": "http://deployer.org",
  "support": {
    "issues": "https://github.com/voodoo-rocks/deployer/issues",
    "source": "https://github.com/voodoo-rocks/deployer",
    "docs": "http://deployer.org/docs"
  },
  "authors": [
    {
      "name": "Alexander Kryshtalev",
      "email": "kryshtalev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Deployer\\": "src/"
    }
  },
  "bin": [
    "bin/dep"
  ],
  "require": {
    "php": ">=5.4.0",
    "elfet/pure": "~1.1",
    "symfony/finder": "~2.6",
    "symfony/console": "~2.6",
    "symfony/yaml": "~2.6",
    "symfony/process": "~2.6",
    "kherge/amend": "~3.0",
    "phpseclib/phpseclib": "~2.0",
    "deployphp/recipes": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5"
  },
  "suggest": {
    "herzult/php-ssh": "For SSH support through native SSH2 extension",
    "ext-sockets": "For parallel deployment"
  }
}
